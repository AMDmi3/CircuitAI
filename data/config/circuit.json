"build_chain": {
	// WARNING: Avoid recursion
	// <category>: factory, nano, store, pylon, energy, defence, bunker, big_gun, radar, sonar, mex, repair
	"energy": {
		// <UnitDef>: {<elements>}
		"cafus": {
			// Available elements:
			// "energy": mex or <boolean>
			// "pylon": <boolean>
			// "porc": <boolean>
			// "terra": <boolean>
			// "hub": [
			//     // chain1
			//     [{<unit1>, <category>, <offset>, <condition>}, {<unit2>, <category>, <offset>, <condition>}, ...],
			//     // chain2
			//     [{...}, {...}, ...],
			//     ...
			// ]
			// <unit>: UnitDef
			// <offset>:
			//     1) [x, z] in South facing, elmos
			//     2) {<direction>: <delta>} - left, right, front, back
			// <condition>: air, no_air, maybe

			// Build pylon in direction of nearby mex cluster
			"pylon": true,

			// Build chain of units
			"hub": [
				[  // chain1
					{"unit": "corrl", "category": "defence", "offset": [-80, -80]},
					{"unit": "corrl", "category": "defence", "offset": {"left": 80}},
					{"unit": "corrl", "category": "defence", "offset": [-70, 70]},
					{"unit": "corrl", "category": "defence", "offset": [-60, 60]},
					{"unit": "corrl", "category": "defence", "offset": {"front": 40}}
				],
				[  // chain2
